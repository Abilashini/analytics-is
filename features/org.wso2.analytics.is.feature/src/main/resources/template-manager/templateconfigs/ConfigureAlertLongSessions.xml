<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<scenarioConfiguration  domain="ISAnalytics" name="ConfigureAlertLongSessions" scenario="ConfigureAlertLongSessions">
    <description>Configure long session duration detection</description>
    <parameterMap>
        <entry>
            <key>sessionDurationThreshold</key>
            <value>900000</value>
        </entry>
        <entry>
            <key>avgPercentageThreshold</key>
            <value>50.0</value>
        </entry>
        <entry>
            <key>numberOfDays</key>
            <value>7</value>
        </entry>
    </parameterMap>
</scenarioConfiguration>